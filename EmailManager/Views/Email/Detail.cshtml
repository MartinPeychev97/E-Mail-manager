@model EmailViewModel

@{
    ViewData["Title"] = "Detail";
}

<h1>Detail</h1>

<table class="greenTable">
    <thead>
        <tr>
            <th>№</th>
            <th>Sender</th>
            <th>ReceiveDate</th>
            <th>Subject</th>
            <th>Body</th>
            <th>EnumStatus</th>
            <th>Attachments</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.LoanId</td>
            <td>@Model.Sender</td>
            <td>@Model.ReceiveDate</td>
            <td>@Model.Subject</td>
            <td>@Model.Body</td>
            <td>@Model.EnumStatus</td>
            <td>@Model.AttachmentsCount</td>
        </tr>
    </tbody>
</table>